{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "PUBGDisc",
    "version": "1.0.0",
    "author": "Pyragon",
    "icon": "source/images/icon.png",
    "icon_gray": "source/images/icon_gray.png",
    "minimum-overwolf-version": "0.92.300.0",
    "description": "PUBG bot to output data to Discord"
  },
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        10906
      ]
    },
    "start_window": "index",
    "windows": {
      "index": {
        "file": "source/index.html",
        "transparent": true,
        "clickthrough": true,
        "resizable": false,
        "show_in_taskbar": true,
        "size": {
          "width": 1150,
          "height": 535
        },
        "start_position": {
          "Top": 10,
          "Left": 10
        }
      }
    },
    "game_events": [
      10906
    ],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            10906
          ]
        },
        "start_minimized": false
      }
    ]
  }
}
